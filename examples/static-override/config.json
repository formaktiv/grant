{
  "defaults": {
    "protocol": "http",
    "host": "localhost:3000"
  },
  "facebook": {
    "key": "APP_ID",
    "secret": "APP_SECRET",
    "scope": [
      "public_profile",
      "email"
    ],
    "callback": "/hello",
    "overrides": {
      "photos": {
        "scope": [
          "user_photos"
        ]
      },
      "videos": {
        "scope": [
          "user_videos"
        ]
      }
    }
  },
  "twitter": {
    "key": "CONSUMER_KEY",
    "secret": "CONSUMER_SECRET",
    "callback": "/hi",
    "overrides": {
      "write": {
        "key": "CONSUMER_KEY",
        "secret": "CONSUMER_SECRET"
      }
    }
  }
}
